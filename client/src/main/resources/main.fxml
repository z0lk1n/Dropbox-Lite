<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerMain">
    <children>
        <Label layoutX="15.0" layoutY="54.0" text="List all files:">
            <font>
                <Font name="Verdana" size="24.0" />
            </font>
        </Label>
        <ListView fx:id="filesListView" layoutX="15.0" layoutY="84.0" onMouseClicked="#focusFile" prefHeight="307.0" prefWidth="427.0" />
        <Button fx:id="uploadBtn" layoutX="174.0" layoutY="14.0" mnemonicParsing="false" onAction="#upload" prefHeight="26.0" prefWidth="107.0" text="Upload" />
        <Button fx:id="downloadBtn" layoutX="78.0" layoutY="14.0" mnemonicParsing="false" onAction="#download" text="Download" />
        <Button fx:id="deleteBtn" layoutX="295.0" layoutY="14.0" mnemonicParsing="false" onAction="#delete" prefHeight="26.0" prefWidth="107.0" text="Delete" />
    </children>
</AnchorPane>
