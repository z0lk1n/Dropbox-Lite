<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="455.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerTable">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="List all files:">
            <font>
                <Font name="Verdana" size="24.0" />
            </font>
        </Label>
        <TableView fx:id="tableData" layoutX="15.0" layoutY="84.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="307.0" prefWidth="427.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="250.0" text="Name" />
                <TableColumn fx:id="sizeColumn" prefWidth="254.0" text="Size" />
                <TableColumn fx:id="dateColumn" prefWidth="125.0" text="Date" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button layoutX="15.0" layoutY="58.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="107.0" text="Upload file" />
        <Button layoutX="121.0" layoutY="58.0" mnemonicParsing="false" text="Download file" />
        <Button layoutX="228.0" layoutY="58.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="107.0" text="Delete file" />
        <Button layoutX="335.0" layoutY="58.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="107.0" text="Change file" />
    </children>
</AnchorPane>
